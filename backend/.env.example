# Origem permitida para requisições CORS (frontend)
CORS_ORIGIN=http://localhost:5173

# Chave secreta para assinatura de tokens JWT
JWT_SECRET=

# Chave de API do OpenRouter
OPENROUTER_API_KEY=sk-or-v1-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# URI de conexão com o MongoDB (container Docker)
MONGO_URI="mongodb://mongo:27017/documentosDB"

# Nome do bucket para armazenamento de arquivos
MINIO_BUCKET_NAME=categorizar-docs-grupo1

# Configurações de conexão e credenciais do MinIO
MINIO_ENDPOINT=minio
MINIO_PORT=9000
MINIO_USE_SSL=false
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin